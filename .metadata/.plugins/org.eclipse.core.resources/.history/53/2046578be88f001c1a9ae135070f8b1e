package lab8;

import java.util.*;

public class Excercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Task t1 = new Task();
		
		Timer t = new Timer();
		
		t.schedule(t1, 10000);
		t.schedule(t1, 1000, 2000); //  executes for every 10 seconds
		t.cancel();

	}

}
